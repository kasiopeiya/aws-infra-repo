    "@aws-sdk/util-dynamodb": "^3.515.0",
    "@middy/core": "^3.6.2",
    "aws-cdk-lib": "2.128.0",
    "aws-sdk": "^2.1560.0",
    "axios": "^1.6.7",
    "cdk-aws-lambda-powertools-layer": "^3.7.0",
    "constructs": "^10.3.0",
    "esbuild": "^0.20.2",
    "install": "^0.13.0",
    "npm": "^10.5.2",
    "undici": "^6.6.2",
    "winston": "^3.12.0"
