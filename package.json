{
  "name": "infra-repo",
  "version": "0.1.0",
  "bin": {
    "infra-repo": "bin/infra-repo.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
